                    }
                  },
                  {
                    "value": " ",
                    "textStyle": {
                      "fontWeight": "bold"
                    }
                  },
                  {
                    "value": "Severity Mix Index ranges from "
                  },
                  {
                    "value": {
                      "propertyIdentifier": {
                        "objectName": "values",
                        "propertyName": "expr"
                      },
                      "selector": {
                        "id": "smi_min"
                      }
                    }
