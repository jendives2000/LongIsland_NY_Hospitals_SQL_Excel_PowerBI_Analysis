                  {
                    "value": " to "
                  },
                  {
                    "value": {
                      "propertyIdentifier": {
                        "objectName": "values",
                        "propertyName": "expr"
                      },
                      "selector": {
                        "id": "Value"
                      }
                    }
                  },
                  {
                    "value": " across all selected hospitals, with a peer-group median of "
                  },
                  {
                    "value": {
                      "propertyIdentifier": {
                        "objectName": "values",
                        "propertyName": "expr"
                      },
                      "selector": {
                        "id": "median_smi"
                      }
                    }
                  },
                  {
                    "value": ". And "
                  },
                  {
                    "value": {
                      "propertyIdentifier": {
                        "objectName": "values",
                        "propertyName": "expr"
                      },
                      "selector": {
                        "id": "above_cnt"
                      }
                    }
                  },
                  {
                    "value": " of "
                  },
                  {
                    "value": {
                      "propertyIdentifier": {
                        "objectName": "values",
                        "propertyName": "expr"
                      },
                      "selector": {
                        "id": "total_cnt"
                      }
                    }
                  },
                  {
                    "value": " facilities operate above that peer-group median severity, with 7 of 23 operating above their own peer-group severity median. This indicates "
                  },
                  {
                    "value": "moderate but controlled variation",
                    "textStyle": {
                      "fontWeight": "bold"
                    }
                  },
                  {
                    "value": " in baseline patient complexity across the network. "
                  },
                  {
                    "value": "Outliers exist, but widespread acuity distortion does not",
                    "textStyle": {
                      "fontWeight": "bold"
                    }
                  },
                  {
                    "value": "."
                  }
                ]
              },
              {
                "textRuns": [
                  {
                    "value": ""
                  }
                ]
              },
              {
                "textRuns": [
                  {
                    "value": "IMPACT: ",
                    "textStyle": {
                      "fontWeight": "bold",
                      "textDecoration": "underline"
                    }
                  },
                  {
                    "value": "Because severity levels are "
                  },
                  {
                    "value": "tightly clustered within peer groups",
                    "textStyle": {
                      "fontWeight": "bold"
                    }
                  },
                  {
                    "value": ", differences observed in "
                  },
                  {
                    "value": "LOS, mortality, and cost",
                    "textStyle": {
                      "fontWeight": "bold"
                    }
                  },
                  {
                    "value": " are "
                  },
                  {
                    "value": "unlikely to be driven by case-mix alone",
                    "textStyle": {
                      "fontWeight": "bold"
                    }
                  },
                  {
                    "value": "."
                  }
                ]
              },
              {
                "textRuns": [
                  {
                    "value": "This strengthens confidence that downstream performance variation reflects "
                  },
                  {
                    "value": "operational practices, throughput management, or care pathways",
                    "textStyle": {
                      "fontWeight": "bold"
                    }
                  },
                  {
                    "value": ", rather than unavoidable acuity differences."
                  }
                ]
              },
              {
                "textRuns": [
                  {
                    "value": ""
                  }
                ]
              },
              {
                "textRuns": [
                  {
                    "value": "ACTION:",
                    "textStyle": {
                      "fontWeight": "bold",
                      "textDecoration": "underline"
                    }
                  },
                  {
                    "value": " ",
                    "textStyle": {
                      "fontWeight": "bold"
                    }
                  },
                  {
                    "value": "Use this severity baseline to:"
                  }
                ]
              },
              {
                "textRuns": [
                  {
                    "value": "Normalize comparisons",
                    "textStyle": {
                      "fontWeight": "bold"
                    }
                  },
                  {
                    "value": " in LOS, cost, and outcome KPIs without heavy risk adjustment"
                  }
                ],
                "listType": "bullet"
              },
              {
                "textRuns": [
                  {
                    "value": "Prioritize operational deep-dives",
                    "textStyle": {
                      "fontWeight": "bold"
                    }
                  },
                  {
                    "value": " (flow, discharge efficiency, service-line practices) at facilities diverging from peer norms"
                  }
                ],
                "listType": "bullet"
              },
              {
                "textRuns": [
                  {
                    "value": "Treat "
                  },
                  {
                    "value": "outlier performance",
                    "textStyle": {
                      "fontWeight": "bold"
                    }
                  },
                  {
                    "value": " as a signal for process review rather than case-mix justification"
                  }
                ],
                "listType": "bullet"
              }
            ]
          }
        }
